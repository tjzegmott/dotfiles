  HostName arc.gb.nrao.edu
  ProxyJump gbo
  User tzegmott
  ForwardAgent yes
  SendEnv FRB_MASTER_ACCESS_TOKEN
  SendEnv FRB_MASTER_REFRESH_TOKEN

